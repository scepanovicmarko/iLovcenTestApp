<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Field.
    -->
    <changeSet id="20190814073157-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="policy_id"
                                 baseTableName="field"
                                 constraintName="fk_field_policy_id"
                                 referencedColumnNames="id"
                                 referencedTableName="policy"/>

        <addForeignKeyConstraint baseColumnNames="field_group_id"
                                 baseTableName="field"
                                 constraintName="fk_field_field_group_id"
                                 referencedColumnNames="id"
                                 referencedTableName="field_group"/>

    </changeSet>
</databaseChangeLog>
